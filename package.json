{
  "name": "rwanda-cancer-relief",
  "version": "0.0.1",
  "private": true,
  "description": "Healthcare management platform for Rwanda Cancer Relief",
  "keywords": [
    "healthcare",
    "cancer-relief",
    "nextjs",
    "supabase",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/rwanda-cancer-relief.git"
  },
  "author": "Rwanda Cancer Relief Development Team",
  "license": "UNLICENSED",
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:unit": "vitest run --dir apps/web/__tests__/unit",
    "test:integration": "vitest run --dir apps/web/__tests__/integration",
    "test:e2e": "playwright test",
    "test:coverage": "vitest run --coverage",
    "seed:dashboard": "tsx scripts/seed-dashboard-data.ts",
    "test:dashboard-data": "tsx scripts/test-dashboard-data.ts",
    "backfill:avatars": "tsx scripts/backfill-counselor-avatars.ts",
    "configure:resend": "tsx scripts/configure-resend-smtp.ts"
  },
  "devDependencies": {
    "@supabase/supabase-js": "^2.80.0",
    "@workspace/eslint-config": "workspace:*",
    "@workspace/typescript-config": "workspace:*",
    "baseline-browser-mapping": "^2.8.32",
    "eslint": "^9.32.0",
    "prettier": "^3.6.2",
    "shadcn": "^3.5.0",
    "tsx": "^4.20.6",
    "turbo": "^2.5.5",
    "typescript": "5.7.3"
  },
  "packageManager": "pnpm@10.4.1",
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.3",
    "animate-ui": "^0.0.4",
    "class-variance-authority": "^0.7.1",
    "lucide-react": "^0.475.0",
    "react-day-picker": "^9.11.1"
  },
  "pnpm": {
    "overrides": {
      "@types/react": "19.2.2",
      "@types/react-dom": "19.2.2"
    },
    "onlyBuiltDependencies": [
      "@tailwindcss/oxide",
      "@vercel/speed-insights",
      "core-js",
      "core-js-pure",
      "esbuild",
      "msw",
      "sharp"
    ]
  }
}
