{
  "mcpServers": {
    "shadcn": {
      "command": "npx",
      "args": [
        "shadcn@latest",
        "mcp"
      ]
    },
    "next-devtools": {
      "command": "npx",
      "args": [
        "-y",
        "next-devtools-mcp@latest"
      ]
    },
    "supabase": {
      "url": "https://mcp.supabase.com/mcp"
    },
    "assistant-ui": {
      "command": "npx",
      "args": [
        "-y",
        "@assistant-ui/mcp-docs-server"
      ]
    },
    "ai-elements": {
      "command": "npx",
      "args": [
        "-y",
        "mcp-remote",
        "https://registry.ai-sdk.dev/api/mcp"
      ]
    }
  }
}